{
  "name": "hr-dapp-contracts",
  "private": true,
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "generate-types": "typechain --target=truffle-v5 'build/contracts/*.json'",
    "migrate": "tsc -p ./tsconfig.migrate.json --outDir ./migrations && truffle migrate",
    "typecheck": "truffle compile && pnpm generate-types && tsc --noEmit"
  },
  "devDependencies": {
    "@typechain/truffle-v5": "^8.0.1",
    "@types/bn.js": "^4.11.6",
    "@types/chai": "^4.3.3",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.8.3",
    "@types/web3": "^1.2.2",
    "truffle": "^5.6.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.4",
    "web3": "^1",
    "web3-core": "^1",
    "web3-eth-contract": "^1",
    "web3-utils": "^1"
  },
  "dependencies": {
    "@openzeppelin/contract-loader": "^0.6.3",
    "@openzeppelin/contracts": "^4.7.3",
    "@truffle/hdwallet-provider": "^1.7.0"
  }
}
